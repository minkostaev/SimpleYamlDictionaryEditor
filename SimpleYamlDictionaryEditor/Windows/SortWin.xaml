<Window x:Class="SimpleYamlDictionaryEditor.Windows.SortWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimpleYamlDictionaryEditor.Windows"
        mc:Ignorable="d" Title="Sort"
        Height="140" Width="400"
        MinHeight="140" MinWidth="400">
    <Grid>
        <ComboBox Name="cbYamSort" Margin="10,10,10,0" VerticalAlignment="Top"/>
        <Grid x:Name="grdSort" Height="50" Width="120" Margin="0,0,10,10" VerticalAlignment="Bottom" HorizontalAlignment="Right" />
        <Label Name="lblInfo" Content="lbl" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
