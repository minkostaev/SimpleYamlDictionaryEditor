<Window x:Class="SimpleYamlDictionaryEditor.Windows.ManuallyWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimpleYamlDictionaryEditor.Windows"
        mc:Ignorable="d" Title="Manually"
        Height="450" Width="800"
        MinHeight="450" MinWidth="800">
    <Window.Resources>
        
    </Window.Resources>
    <Grid>
        <Label Content="File A:" HorizontalAlignment="Left" Margin="6,8,0,0" VerticalAlignment="Top"/>
        <Label Content="File B:" HorizontalAlignment="Left" Margin="6,38,0,0" VerticalAlignment="Top"/>

        <ComboBox x:Name="cbFileA" Margin="50,10,310,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cbFileB" Margin="50,40,310,0" VerticalAlignment="Top"/>

        <Grid x:Name="grdSaveA" Height="50" Width="140" Margin="0,10,160,0" VerticalAlignment="Top" HorizontalAlignment="Right" />
        <Grid x:Name="grdSaveB" Height="50" Width="140" Margin="0,10,10,0" VerticalAlignment="Top" HorizontalAlignment="Right" />

        <DataGrid x:Name="dtGrd" Margin="10,70,10,10" />

    </Grid>
</Window>
