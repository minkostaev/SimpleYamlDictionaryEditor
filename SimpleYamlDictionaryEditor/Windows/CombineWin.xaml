<Window x:Class="SimpleYamlDictionaryEditor.Windows.CombineWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimpleYamlDictionaryEditor.Windows"
        mc:Ignorable="d" Title="Combine"
        Height="170" Width="400"
        MinHeight="170" MinWidth="400">
    <Grid>
        <Label Content="File A:" HorizontalAlignment="Left" Margin="6,8,0,0" VerticalAlignment="Top"/>
        <Label Content="File B:" HorizontalAlignment="Left" Margin="6,38,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblInfo" Content="lbl" HorizontalAlignment="Left" Margin="6,70,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cbReplaceA" Margin="50,10,10,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cbReplaceB" Margin="50,40,10,0" VerticalAlignment="Top"/>
        <Grid x:Name="grdReplace" Height="50" Width="140" Margin="0,0,10,10" VerticalAlignment="Bottom" HorizontalAlignment="Right" />
    </Grid>
</Window>
